{
    "define":[
        {
            "type":"hulft-snd",
            "input":{
                "record-start":"^SNDFILE",
                "record-end":"^END$",
                "record-item":[
                    "^SNDFILE=(.+)$",
                    "^FILENAME=(.+)$",
                    "^GRPID=(.+)$",
                    "^COMMENT=(.+)$"
                ]
            },
            "output":{
                "header":"SNDFILE,FILENAME,GRPID,COMMENT",
                "record":"1,2,3,4"
            }
        },
        {
            "type":"hulft-rcv",
            "input":{
                "record-start":"^RCVFILE",
                "record-end":"^END$",
                "record-item":[
                    "^RCVFILE=(.+)$",
                    "^FILENAME=(.+)$",
                    "^GRPID=(.+)$",
                    "^COMMENT=(.+)$"
                ]
            },
            "output":{
                "header":"RCVFILE,FILENAME,GRPID,COMMENT",
                "record":"1,2,3,4"
            }
        },
        {
            "type":"hulft-hst",
            "input":{
                "record-start":"^HOST",
                "record-end":"^END$",
                "record-item":[
                    "^HOST=(.+)$",
                    "^HOSTTYPE=(.+)$",
                    "^KCODETYPE=(.+)$",
                    "^COMMENT=(.+)$"
                ]
            },
            "output":{
                "header":"HOST,HOSTTYPE,KCODETYPE,COMMENT",
                "record":"1,2,3,4"
            }
        },
        {
            "type":"hulft-tgrp",
            "input":{
                "record-start":"^GRP",
                "record-end":"^END$",
                "record-item":[
                    "^GRP=(.+)$",
                    "^FORMAT DEF(.+)DEFEND$",
                    "^COMMENT=(.+)$"
                ]
            },
            "output":{
                "header":"GRP,HOSTS,COMMENT",
                "record":"1,2,3"
            }
        }
    ]
}